{
    "name": "eviweb/composer-test-utils",
    "description": "Set of utilities to help testing Composer related components.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Villard",
            "email": "dev@eviweb.fr"
        }
    ],
    "require": {
        "composer/composer": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Eviweb\\Composer\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eviweb\\Composer\\Testing\\": "tests/unit/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}